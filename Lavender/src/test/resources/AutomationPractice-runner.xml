<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Automation Practice smoke">
    <parameter name="useCloudEnv" value="false"></parameter>
    <parameter name="envName" value="browserstack"></parameter>
    <parameter name="os" value="windows"></parameter>
    <parameter name="osVersion" value="11"></parameter>
    <parameter name="browserName" value="chrome"></parameter>
    <parameter name="browserVersion" value="102"></parameter>
    <parameter name="url" value="https://www.automationpractice.com"></parameter>

    <test name="Automation Practice">
        <classes>
            <class name= "apps.AutomationPractice.SigninTest"></class>
            <class name="apps.AutomationPractice.AccessingWomenTest"></class>
            <class name="apps.AutomationPractice.AccessingAccountTest"></class>
            <class name="apps.AutomationPractice.SearchTest"></class>
            <class name="apps.AutomationPractice.WishListTest"></class>
            <class name="apps.AutomationPractice.CartTest"></class>
            <class name="apps.AutomationPractice.SendingToAFriendTest"></class>
            <class name="apps.AutomationPractice.WritingReviewTest"></class>
        </classes>
    </test>

</suite>