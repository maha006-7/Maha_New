<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="DemoBlaze Testing">
    <parameter name="useCloudEnv" value="false"></parameter>
    <parameter name="envName" value="browserstack"></parameter>
    <parameter name="os" value="mac"></parameter>
    <parameter name="osVersion" value="12"></parameter>
    <parameter name="browserName" value="chrome"></parameter>
    <parameter name="browserVersion" value="102"></parameter>
    <parameter name="url" value="https://demoblaze.com/"></parameter>

    <test name="Verify Test">
        <classes>
            <class name="apps.DemoBlaze.Verify"></class>
        </classes>
    </test>

    <test name="RegisterLogIn">
        <classes>
            <class name="apps.DemoBlaze.RegisterLogIn"></class>
        </classes>
    </test>

</suite>

