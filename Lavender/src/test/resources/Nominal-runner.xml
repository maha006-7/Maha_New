<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Nominal Testing">
    <parameter name="useCloudEnv" value="false"></parameter>
    <parameter name="envName" value="browserstack"></parameter>
    <parameter name="os" value="mac"></parameter>
    <parameter name="osVersion" value="12"></parameter>
    <parameter name="browserName" value="chrome"></parameter>
    <parameter name="browserVersion" value="102"></parameter>
    <parameter name="url" value="https://nominalx.com/"></parameter>

    <test name="User Login Testing">
        <classes>
            <class name="apps.Nominalx.Nominal"></class>
        </classes>
    </test>

    <test name="User Purchase Testing">
        <classes>
            <class name="Nominalx.(whateverclassicreate)"></class>
        </classes>
    </test>

</suite>
